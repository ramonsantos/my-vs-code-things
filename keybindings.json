[
  {
    "key": "ctrl+'",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "f11",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+o",
    "command": "-workbench.action.files.openFile",
    "when": "true"
  },
  {
    "key": "ctrl+shift+v",
    "command": "swagger.preview",
    "when": "editorLangId == 'yaml'"
  },
  {
    "key": "shift+alt+p",
    "command": "-swagger.preview",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "ctrl+shift+t",
    "command": "extension.runOpenSpec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "alt+meta+o",
    "command": "-extension.runOpenSpec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "ctrl+f6",
    "command": "extension.runLineOnRspec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "meta+l",
    "command": "-extension.runLineOnRspec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "ctrl+f7",
    "command": "extension.runFileOnRspec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "alt+meta+l",
    "command": "-extension.runFileOnRspec",
    "when": "editorLangId == 'ruby'"
  },
  {
    "key": "ctrl+f8",
    "command": "code-runner.run"
  },
  {
    "key": "ctrl+alt+n",
    "command": "-code-runner.run"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.toggleZenMode",
    "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "ctrl+k z",
    "command": "-workbench.action.toggleZenMode",
    "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "ctrl+\\",
    "command": "-workbench.action.splitEditor"
  },
  {
    "key": "f10",
    "command": "-workbench.action.debug.stepOver",
    "when": "debugState == 'stopped'"
  },
  {
    "key": "f10",
    "command": "editor.action.referenceSearch.trigger"
  },
  {
    "key": "ctrl+f4",
    "command": "toggle",
    // "when": "editorTextFocus",
    "args": {
      "id": "codelens",
      "value": [
        {
          "editor.codeLens": true
        },
        {
          "editor.codeLens": false
        }
      ]
    }
  },
  {
    "key": "ctrl+shift+f1",
    "command": "editor.action.inspectTMScopes"
  },
  {
    "key": "ctrl+shift+m",
    "command": "-workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "ctrl+shift+m",
    "command": "extension.railsLatestMigration"
  },
  {
    "key": "ctrl+shift+n",
    "command": "-workbench.action.newWindow"
  },
  {
    "key": "ctrl+shift+n",
    "command": "rails.fastNavigation",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+r",
    "command": "-rails.fastNavigation",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+s",
    "command": "-gitlens.showQuickRepoStatus",
    "when": "!gitlens:disabled && config.gitlens.keymap == 'alternate'"
  },
  {
    "key": "alt+s",
    "command": "sorbet.copySymbolToClipboard"
  },
]
