{
	"bracketPairColorizer.colorMode": "Consecutive",
	"bracketPairColorizer.consecutivePairColors": [
		"()",
		"[]",
		"{}",
		[
			"#F92672"
		],
		"Red"
	],
	"coverage-gutters.coverageReportFileName": "info.lcov",
	"coverage-gutters.showLineCoverage": true,
	"editor.detectIndentation": false,
	"editor.fontSize": 16,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": false,
	"editor.minimap.enabled": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.renderWhitespace": "all",
	"editor.rulers": [
		80,
		115
	],
	"editor.snippetSuggestions": "top",
	"editor.tabSize": 2,
	"explorer.confirmDelete": false,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"gitlens.advanced.messages": {
		"suppressResultsExplorerNotice": true,
		"suppressShowKeyBindingsNotice": true
	},
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "annotation",
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.lineHistory.enabled": true,
	"ruby.intellisense": "rubyLocate",
	"ruby.rubocop.configFilePath": "/home/ramonsantos/.rubocop.yml",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.cursorStyle": "underline",
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.fontFamily": "Hack Nerd Font",
	"terminal.integrated.fontSize": 15,
	"terminal.integrated.shell.linux": "/bin/zsh",
	"window.titleBarStyle": "custom",
	"window.zoomLevel": 0,
	"workbench.colorCustomizations": {
		"editorCursor.foreground": "#F5FFFA",
		"editorIndentGuide.activeBackground": "#999",
		"editorRuler.foreground": "#F92672"
	},
	"workbench.colorTheme": "Monokai",
	"workbench.iconTheme": "vscode-great-icons",
	"workbench.startupEditor": "newUntitledFile",
	"[python]": {
		"editor.formatOnSave": true,
		"editor.tabSize": 4,
	},
	"[json]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
		"files.insertFinalNewline": false,
  }
}
